                            </Border.Background>

                            <StackPanel Orientation="Horizontal">
                                <Canvas Width="12" Margin="5,0,0,0" Height="14" Clip="F1 M 0,0L 14,0L 14,14L 0,14L 0,0">
                                    <Path Width="12" Height="14" Stretch="Fill" Fill="#6eb6e6" 
